<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="stud.subh.hibernate.ex20.dao.Account">
		<id name="accno"/>
		<property name="name"/>
		<property name="balance"/>
		<many-to-one name="atmCard" class="stud.subh.hibernate.ex20.dao.ATMCard" column="cno" unique="true" cascade="all"/>
	</class>
</hibernate-mapping>
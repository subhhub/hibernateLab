<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping default-lazy="false">	<!-- default lazy is true -->	
	<class name="stud.subh.hibernate.ex1.dao.Account" table="ACCOUNT" >
		<id name="acno" column="ACCNO">
			<!-- <generator class="org.hibernate.id.IncrementGenerator"/> -->
		</id>
		<property name="nm" column="NAME" />
		<property name="bal" column="BALANCE"/>
	</class>
</hibernate-mapping>